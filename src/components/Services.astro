---
import Service from "./templates/Service.astro";

const {titulo, subtitle, services} = Astro.props;
---

<section id="services" class="bg-secondary seccion">
  <div class="services container" style="padding: 6em 0 6em">
    <div class="text-center" style="padding-bottom: 4em">
      <h1 class="text-info title-about fw-2">{titulo}</h1>
      <hr
        class="separador text-primary border border-primary mb-4"
        style="width: 10%; margin: auto"
      />
      <p style="width: 50%; margin: auto" class="fw-0"> {subtitle}
      </p>
    </div>

    <div class="row gy-4 mb-5">
      
      {Object.keys(services).map(key => {
        const service = services[key];
        return <Service key={key} {...service} />;
      })}

